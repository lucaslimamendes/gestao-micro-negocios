<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="780.0" style="-fx-background-color: #393351;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gestao.micro.negocios.controller.CustomerController">
   <children>
      <TableView fx:id="customerTable" layoutY="180.0" prefHeight="370.0" prefWidth="780.0">
